@model Asignatura

@{
    ViewData["Title"] = "Información Asignatura";
}

<strong>Desde la vista normal</strong>

<h2>Asignatura: @ViewData["Title"]</h2>

<p>
    Nombre asignatura: @Model.Nombre
    Id: @Model.Id
</p>

<strong>Desde la vista parcial</strong>
<partial name="ObjetoEscuelaSimple"/>

<p><strong><i>@ViewBag.Fecha</i></strong></p>