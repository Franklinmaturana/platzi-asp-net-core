@model List<Alumno>

@{
    ViewData["Title"] = "Información Alumno";
}

<strong>Desde la vista normal</strong>

<h2>Asignatura: @ViewData["Title"]</h2>
@* @foreach (var alumno  in Model)
    {
    <partial name="ObjetoEscuelaSimple" model="alumno"/> 
    } *@


<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var alumno in Model)
        {
        <tr>
            <td>@alumno.Nombre</td>
            <td>@alumno.Id</td>
        </tr>
        }
    </tbody>
</table>


<p><strong><i>@ViewBag.Fecha</i></strong></p>